msgid "Available"
msgstr "Beschikbaar"

#: ../js/ui/statusMenu.js:112
msgid "Busy"
msgstr "Bezig"

#: ../js/ui/statusMenu.js:117
msgid "Invisible"
msgstr "Onzichtbaar"

