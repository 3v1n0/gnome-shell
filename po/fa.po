#. i.e. "May 25, 2:30 pm"
#: js/misc/util.js:238
#, no-c-format
msgid "%B %d, %l∶%M %p"
msgstr "%Od %B، %OH:%OM"

#. Translators: this is the month name, day number, year
#. number followed by a time string in 12h format.
#. i.e. "May 25 2012, 2:30 pm"
#: js/misc/util.js:244
#, no-c-format
msgid "%B %d %Y, %l∶%M %p"
msgstr "%Od %B، %OH:%OM"

